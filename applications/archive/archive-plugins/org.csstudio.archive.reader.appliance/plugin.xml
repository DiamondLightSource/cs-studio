          category="org.csstudio.platform.ui.css.applications.trends"
          class="org.csstudio.archive.reader.appliance.PreferencePage"
          id="org.csstudio.archive.reader.appliance.preferences"
          name="Appliance Archiver Reader">
      </page>
  </extension>
</plugin>
